#include <iostream>

using namespace std;

int main()
{
    float base, altura, area;
    cout<<"Ingresar el valor de la BASE del paralelogramo: ";
    cin>>base;
    cout<<"Ingresar la ALTURA: ";
    cin>>altura;
    area=base*altura;
    cout<<"El area es: "<<endl;
    cout<<area;

    return 0;
}
